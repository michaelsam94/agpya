<?xml version="1.0" encoding="utf-8"?>
<manifest package="michael.com.agpya"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="michael.com.agpya.AgpyaApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="michael.com.agpya.PrayerActivity"
            android:label="@string/title_activity_prayer"
            android:parentActivityName="michael.com.agpya.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="michael.com.agpya.MainActivity" />
        </activity>
        <activity android:name="michael.com.agpya.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>